{
  "workbench.colorTheme": "Dark (Molokai)",
  "workbench.iconTheme": "vscode-icons",
  "workbench.startupEditor": "newUntitledFile",
  "editor.smoothScrolling": true,
  "editor.renderWhitespace": "all",
  "editor.tabSize": 2,
  "editor.letterSpacing": 0.5,
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.fontSize": 15,
  "prettier.singleQuote": true,
  "window.zoomLevel": 1,
  "javascript.preferences.quoteStyle": "single",
  "[javascript]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
      "source.fixAll.stylelint": true,
      "source.fixAll": true
    }
  },
  "[javascriptreact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true,
      "source.fixAll.stylelint": true,
      "source.fixAll": true
    }
  },
  "emmet.syntaxProfiles": {
    "javascript": "jsx"
  },
  "emmet.includeLanguages": {
    "javascript": "javacriptreact"
  },
  "bracketPairColorizer.consecutivePairColors": [
    "()",
    "[]",
    "{}",
    ["#4ceb34", "#ddff00", "#33ccff"],
    "Red"
  ],
  "terminal.integrated.fontSize": 15,
  "terminal.integrated.fontWeight": "bold",
  "editor.fontFamily": "'Ubuntu Mono'",
  "workbench.colorCustomizations": {
    "[Dark (Molokai)]": {
      "statusBar.background": "#3d3d3d",
      "statusBar.noFolderBackground": "#3d3d3d",
      "gitDecoration.modifiedResourceForeground": "#7bfd7b",
      "gitDecoration.untrackedResourceForeground": "#F92672",
      "diffEditor.insertedTextBackground": "#135310",
      "diffEditor.removedTextBackground": "#6b0b0b",
      "diffEditor.insertedTextBorder": "#00ff55",
      "diffEditor.removedTextBorder": "#ff0000"
    }
  }
}
